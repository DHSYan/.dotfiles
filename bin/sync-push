#!/bin/sh

cd ~/Concept-Notes
git status
git add .
echo "Notes Commit Msg: "
read -r notes
git commit -m "{$notes}"
git push origin main

cd ~/Ansible-linux
git status
git add .
echo "Ansible Commit Msg: "
read -r Ansible
git commit -m "{$Ansible}"
git push origin main

cd ~/.dotfiles
git status
git add .
echo "Dotfiles Commit Msg: "
read -r dotfiles
git commit -m "{$dotfiles}"
git push origin main
